<?xml version='1.0' encoding='utf-8'?>
<odoo>



  <record id="importexcel_action" model="ir.actions.act_window">
    <field name="name">Import Excel</field>
    <field name="type">ir.actions.act_window</field>
    <field name="res_model">importexcel</field>
    <field name="view_mode">tree,form</field>
    <field name="view_type">form</field>
  </record>

  <record id="exportexcel_action" model="ir.actions.act_window">
    <field name="name">Export Excel</field>
    <field name="type">ir.actions.act_window</field>
    <field name="res_model">exportexcel</field>
    <field name="view_mode">tree,form</field>
    <field name="view_type">form</field>
  </record>

  <menuitem id="importexcel.menu_importexcel"
            name="importexcel"
            parent="sale.product_menu_catalog"
            action="importexcel_action"
            sequence="100"/>

  <menuitem id="importexcel.menu_exportexcel"
            name="exportexcel"
            parent="sale.product_menu_catalog"
            action="exportexcel_action"
            sequence="200"/>

</odoo>
